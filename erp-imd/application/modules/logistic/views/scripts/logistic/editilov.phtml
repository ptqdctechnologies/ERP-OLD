    function delList()
    {
        Ext.MessageBox.confirm('Confirm', 'Are you sure want to delete this?', function(btn) {
            if (btn == 'yes')
            {
                var rec = Ext.getCmp('ilov-grid').getSelectionModel().getSelections();
                var a = store.getAt(store.findExact('id', rec[0].data['id']));
                var b = storeIlov.getAt(storeIlov.findExact('id', rec[0].data['id']));
                if (b != undefined)
                {
                    storeIlov.remove(b);
                    Ext.getCmp('ilov-grid').getView().refresh();
                    Ext.getCmp('boq3-grid').getView().refresh();
                }
                clearForm();
            }

        });
    }

