                            success: function(resp) {
                                var returnData = Ext.util.JSON.decode(resp.responseText);
                                var lastID = store.getCount();

<?php if ($this->cancel) { ?>
                                    var selisih = Math.abs(parseFloat(returnData.posts['totalPR']) - parseFloat(stores.data['qty']));
