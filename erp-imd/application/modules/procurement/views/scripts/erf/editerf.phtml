    function getERFRecord(workid, kode_brg, isEdit)
    {
        //Clear filter
        storeERF.clearFilter();
        storeERF.filterBy(function(record, id) {
            if (!isEdit)
            {
                if (record.get('workid') == workid.toString() && record.get('kode_brg') == kode_brg.toString())
                {
                    return record.get('workid');
                }
            }
            else
            {
                if (record.get('workid') == workid.toString() && (record.get('status') != '' || record.get('status') != undefined))
                {
                    return record.get('workid');
                }
            }
        });
        if (storeERF.getCount() > 0)
        {
            var rec = storeERF.getAt(0);
        }

        storeERF.clearFilter();
        return rec;
    }

    function getOriRecordForERF(workid)
    {
        //Clear filter
        store.clearFilter();
        store.filterBy(function(record, id) {
            return record.get('workid') == workid;
        });
        if (store.getCount() > 0)
        {
            var rec = store.getAt(0);
        }

        store.clearFilter();
        return rec;
    }


