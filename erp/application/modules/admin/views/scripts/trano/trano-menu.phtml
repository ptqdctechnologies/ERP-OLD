<script type="text/javascript">

    showTranoType = function(){
        myPanel = Ext.getCmp('abs-budget-panel');
        myPanel.body.load({
            url: '/admin/trano/trano-type',
            scripts : true
        });
    };
    showTranoNumber = function(){
        myPanel = Ext.getCmp('abs-budget-panel');
        myPanel.body.load({
            url: '/admin/trano/trano-number',
            scripts : true
        });
    };
    var windowForm =  ({

        title: 'Transaction Number (Trano) Management',
        id: 'abs-budget-panel',
        layout: 'fit',
        bodyCfg : { cls:'xpanel-body-table' , style: {'overflow':'auto'}},
        bodyStyle: 'padding:15px;',
        loadMask: true,
        minWidth: 300,
        minHeight: 200,
        tbar: [
            {
                xtype: 'button',
                text: 'Trano Type Manager',
                cls:"x-btn-text-icon" ,
                icon:"/images/icons/fam/action_paste.gif",
                handler:showTranoType
            },
            {
                xtype: 'button',
                text: 'Trano Number Manager',
                cls:"x-btn-text-icon" ,
                icon:"/images/icons/fam/action_paste.gif",
                handler:showTranoNumber
            }
        ]
    });
    viewPort = Ext.getCmp('viewport');

    centerRegionTab = viewPort.getComponent('content-panel');
    centerRegionTab.items.each(function(c){centerRegionTab.remove(c);});
    centerRegionTab.add(windowForm);
    panelBaru = Ext.getCmp('abs-budget-panel');
    centerRegionTab.layout.setActiveItem('abs-budget-panel');
</script>