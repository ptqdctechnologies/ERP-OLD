<script type="text/javascript">

    Ext.onReady(function(){

        Ext.QuickTips.init();

        var specsuppstore = new Ext.data.Store ({

            autoLoad:true,
            url:'/logistic/logisticspecsupplier/getspecsupp',
            reader:new Ext.data.JsonReader ({
                root:'data',
                totalProperty:'total',
                fields:[{
                    name:'id'
                },{
                    name:'subjenisupplier'
                },{
                    name:'subjenissupliernama'
                }]
            })


        })

        var specsuppgrid = new Ext.grid.GridPanel ({

            store:specsuppstore,
            frame:'true',
            title:'Specialist Supplier List',
            renderTo:'list_supplier',
            height:300,
            width:460,
            columns:[new Ext.grid.RowNumberer(),{
                header:'Supplier Specialist',
                dataIndex:'subjenisupplier',
                sortable:true,
                width:120
            },{
                header:'Supplier Specialist Name',
                dataIndex:'subjenissupliernama',
                sortable:true,
                width:340
            }],
            bbar:new Ext.PagingToolbar ({
                id: 'paging-specsupp',
                pageSize: 20,
                store: specsuppstore,
                displayInfo: true,
                displayMsg: 'Displaying data {0} - {1} of {2}',
                emptyMsg: "No data to display"
            })



        })


    })


</script>

    <div id="list_supplier"></div>
