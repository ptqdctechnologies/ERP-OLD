                }]
            },{
                xtype:'combo',
                fieldLabel:'Valuta ',
                labelStyle:'margin-left:10px;width:120',
                width:60,
                store: new Ext.data.Store({
                    autoLoad: true,
                    fields:['name','nilai'],
                    proxy: new Ext.data.HttpProxy({
                        url: '/default/valuta/list'
                    }),
                    reader: new Ext.data.JsonReader({
                        totalProperty: 'count',
