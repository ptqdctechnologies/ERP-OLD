var txtboks = function(posX,posY,widthBox,idName,eventName)
 {
     txtboks.superclass.constructor.call(this, {
            id:idName,
            x:posX,
            y:posY,
            width:widthBox,
            enableKeyEvents:true,
            listeners: {
            keyup: eventName
}
         })

 }

Ext.extend(txtboks,Ext.form.TextField);