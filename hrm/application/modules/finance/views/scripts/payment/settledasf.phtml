function refreshGrid()
{
	Ext.getCmp('asf-grid').enable();
	Ext.getCmp('payment-grid').enable();
    Ext.getCmp('asf-grid').getSelectionModel().clearSelections();
    Ext.getCmp('payment-grid').getSelectionModel().clearSelections();
    Ext.getCmp('asf-grid').getView().refresh();
    Ext.getCmp('payment-grid').getView().refresh();
}

function checkList()
{
    storeASF.clearFilter();
    var gridasf = Ext.getCmp('asf-grid').getSelectionModel().getSelections();
    if (gridasf[0] != undefined)
	    var oriasf = store.getAt(store.findExact('id', gridasf[0].data['id']));
    else
        return false;

        var col = storeASF.queryBy(function(record,id){

         return record.get('workid') == oriasf.data['workid'] && record.get('kode_brg') == oriasf.data['kode_brg'];
            });

    if (col.length == 0)
    {
        return false;
    }
    else
    {
            Ext.MessageBox.show({
            title: 'Error',
            msg: 'Please Use Edit To Update this Item!',
            buttons: Ext.MessageBox.OK,
            icon: Ext.MessageBox.ERROR
            });
            return true;
    }

}

