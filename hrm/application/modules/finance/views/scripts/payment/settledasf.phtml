function insertToASF()
{
            if(checkList())
            {
             clearASFForm();
             clearButton();
             refreshGrid();
            }
            else
            {
             var asf_no = Ext.getCmp('asf_no_text').getValue();

            var gridasf = Ext.getCmp('payment-grid').getSelectionModel().getSelections();

            if (gridasf[0] != undefined)
            {
                var oriasf = storeASF.getAt(storeASF.findExact('id', gridasf[0].data['id']));
            }

            if (oriasf != undefined)
            {

            if (oriasf.data['status'] == 'edit')
            {
                    oriasf.data['status'] = '';
                    oriasf.data['coa_kode'] = Ext.getCmp('coa_kode_text').getValue();
                    oriasf.data['coa_nama'] = Ext.getCmp('coa_nama_text').getValue();
            }

            oriasf = undefined;
//            ori = undefined;
             clearASFForm();
             clearButton();
             refreshGrid();

            }
            else
            {

                var qty = ori.data['qty'].toString().replace(/\$|\,/g,'');
                var harga = ori.data['harga'].toString().replace(/\$|\,/g,'');
                var total = parseFloat(qty*harga);

                var e = new asflist({
                    workid: ori.data['workid'],
                    workname: ori.data['workname'],

                    prj_kode: ori.data['prj_kode'],
                    sit_kode: ori.data['sit_kode'],
                    prj_nama: ori.data['prj_nama'],
                    sit_nama: ori.data['sit_nama'],
                    kode_brg: ori.data['kode_brg'],
                    nama_brg: ori.data['nama_brg'],
                    harga: harga,
                    val_kode: ori.data['val_kode'],
                    uom: ori.data['uom'],
                    id: ori.data['id'],
                    qty: qty,
                    total: total,
                    coa_kode: Ext.getCmp('coa_kode_text').getValue(),
                    coa_nama: Ext.getCmp('coa_nama_text').getValue()


                });

                 storeASF.add(e);
                 ori = undefined;
                 e = undefined;
                 clearASFForm();
                 clearButton();
                 refreshGrid();
            }
        }

}

function delToASF(idBoq,workid)
{
	Ext.MessageBox.confirm('Confirm', 'Are you sure want to delete this?', function(btn){
		if (btn == 'yes')
		{
            var gridasf = Ext.getCmp('payment-grid').getSelectionModel().getSelections();
            var b = storeASF.getAt(storeASF.findExact('id', gridasf[0].data['id']));

				if (b != undefined)
				{
					storeASF.remove(b);
					Ext.getCmp('asf-grid').getView().refresh();
					Ext.getCmp('payment-grid').getView().refresh();
				}
			clearASFForm();
			clearButton();
		}

	});
}

